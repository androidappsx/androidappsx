<?PHP
$data = '';

$pass = 'pass';

if (!empty($_REQUEST) ) {
    if (!empty($_REQUEST[$pass])) {
        if ($_REQUEST[$pass] == '607D8BGU4T') {
            $data = '1|'.md5( $_REQUEST[$pass]).'|successfully logged in!';
        }else{
            $data = '0|'.md5(date('Y-m-d h:i:s').rand(1, 9999)).'|Incorrect Key!!';
        }
    }else{
        $data = '0|'.md5(date('Y-m-d h:i:s').rand(1, 9999)).'|Fill in your key first!!';
    }
}

header('Content-Type: text/plain');
echo $data;